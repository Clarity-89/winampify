{
  "name": "winampify",
  "version": "0.0.2",
  "description": "Winampify is a Web Client for Spotify using Winamp 2 skins",
  "main": "built/winamp.bundle.js",
  "scripts": {
    "buildanddeploy": "npm run build && scp -r built/ remi@remigallego.com:/srv/web/winamp/",
    "deploy": "npm run build && cd built/ && now -e ENV=production && now alias",
    "lint": "eslint .",
    "build": "webpack --config=config/webpack.prod.js",
    "build-library": "webpack --config=config/webpack.library.js",
    "serve": "http-server ./built",
    "start": "webpack-dev-server --config=config/webpack.dev.js --port 8888",
    "weight": "echo 'FIXME: Need to find cache-busted file' || (npm run build && gzip-size built/winamp.js)",
    "test": "jest --coverage",
    "travis-tests": "jest --coverage && npm run lint && npm run build",
    "tdd": "jest --watch",
    "build-skin": "rm skins/base-2.91.wsz && cd skins/base-2.91 && zip -x .* -x 'Skining Updates.txt' -r ../base-2.91.wsz .",
    "skin-info": "unzip -vl skins/base-2.91.wsz"
  },
  "repository": {
    "type": "git",
    "url": " "
  },
  "keywords": [
    "Winamp",
    "HTML5",
    "audio",
    "web-audio-api"
  ],
  "author": "Remi Gallego <remi.gallego@gmail.com>",
  "license": "MIT",
  "homepage": "",
